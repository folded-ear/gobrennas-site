    "@fontsource/geist-sans": "~5.2.5",
    "@graphql-typed-document-node/core": "~3.2.0",
    "@heroui/react": "3.0.0-beta.3",
    "@heroui/styles": "3.0.0-beta.3",
    "clsx": "~2.1.1",
    "framer-motion": "~12.23.12",
    "lucide-react": "~0.487.0",
    "next": "16.1.5",
    "next-client-cookies": "~2.1.0",
    "next-themes": "~0.4.6",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
